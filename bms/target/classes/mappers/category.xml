<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="categoryMapper">
	<select id="selectKoreanNovel" resultType="BookDTO">
		SELECT
				*
		FROM
				BOOK	
			
		WHERE
		 		SORT='한국소설'
	</select>
	<select id="selectenglishNovel" resultType="BookDTO">
		SELECT
				*
		FROM
				BOOK	
			
		WHERE
		 		SORT='영문소설'
	</select>
</mapper>